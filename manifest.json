{
  "manifest_version": 3,
  "name": "RefExtractor",
  "version": "1.0",
  "description": "Extracts reference information from web pages.",
  "permissions": ["activeTab"],

  "action": {
    "default_popup": "popup.html",
    "default_icon": "hello_extensions.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
